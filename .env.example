# Environment Variables Template
# Copy this file to .env.local and fill in your actual values

# ================================
# Firebase Configuration
# ================================
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# ================================
# Supabase Configuration
# ================================
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_anon_key
SUPABASE_SERVICE_KEY=your_service_key

# ================================
# OpenAI Configuration (Phase 2+)
# ================================
OPENAI_API_KEY=sk-proj-...
OPENAI_ORG_ID=org-...

# ================================
# Vehicle Data APIs
# ================================
# NHTSA vPIC (Free - No key required)
NHTSA_API_URL=https://vpic.nhtsa.dot.gov/api/

# CARFAX/AutoCheck (Optional)
CARFAX_API_KEY=your_carfax_key
AUTOCHECK_API_KEY=your_autocheck_key

# ================================
# Diagnostics APIs (Phase 2+)
# ================================
# CarMD
CARMD_PARTNER_TOKEN=your_partner_token
CARMD_AUTH_TOKEN=your_auth_token

# RepairPal
REPAIRPAL_API_KEY=your_repairpal_key

# ================================
# Valuation APIs (Phase 3+)
# ================================
# MarketCheck
MARKETCHECK_API_KEY=your_marketcheck_key

# Black Book
BLACKBOOK_API_KEY=your_blackbook_key
BLACKBOOK_CLIENT_ID=your_client_id
BLACKBOOK_CLIENT_SECRET=your_client_secret

# ================================
# Parts & Customization (Phase 3+)
# ================================
# SEMA Data
SEMA_API_KEY=your_sema_key
SEMA_DATASET_LICENSE=your_dataset_license

# ================================
# Computer Vision (Phase 3+)
# ================================
# Google Cloud Vision
GOOGLE_CLOUD_VISION_API_KEY=your_vision_api_key

# Hugging Face (Custom models)
HUGGINGFACE_API_TOKEN=your_huggingface_token

# ================================
# Maps & Location (Phase 3+)
# ================================
# Google Places API
GOOGLE_PLACES_API_KEY=your_places_api_key

# Mapbox (EV charging stations)
MAPBOX_ACCESS_TOKEN=your_mapbox_token

# ================================
# Payment Processing
# ================================
# Stripe
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Stripe Price IDs
STRIPE_PRICE_PRO_MONTHLY=price_...
STRIPE_PRICE_PRO_YEARLY=price_...
STRIPE_PRICE_MECHANIC_MONTHLY=price_...
STRIPE_PRICE_MECHANIC_YEARLY=price_...
STRIPE_PRICE_DEALER_MONTHLY=price_...

# ================================
# Analytics & Monitoring
# ================================
# Mixpanel
MIXPANEL_TOKEN=your_mixpanel_token

# Sentry (Error tracking)
SENTRY_DSN=https://...@sentry.io/...

# Google Analytics (Web)
GA_MEASUREMENT_ID=G-XXXXXXXXXX

# ================================
# Email & Notifications
# ================================
# SendGrid
SENDGRID_API_KEY=SG....

# Twilio (SMS for MFA)
TWILIO_ACCOUNT_SID=AC...
TWILIO_AUTH_TOKEN=your_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# ================================
# Development Settings
# ================================
NODE_ENV=development
API_TIMEOUT=30000
LOG_LEVEL=debug

# ================================
# Production Settings (Override in .env.production)
# ================================
# NODE_ENV=production
# LOG_LEVEL=error
# API_TIMEOUT=15000
